      <div class="entry">
          <div class="body">
          <div class="main__fullBackground">
          <div class="header__container">
              <header>
                  <img src="<%- header.logoSrc %>" alt="<%- header.imgAlt %>">
                  <p class="header__signUp"><strong><%-header.title %></strong></p>
              </header>
          </div>
          </div>
          <div class="main__heroBackground">
            <div class="main__container">
                <div class="main__titleGroup">
                    <h1><%- main.title %></h1>
                    <h3><%- main.subTitle %></h3>
                </div>
                <div class="signUp__container" id="waitingList">
                    <h4><%- signUp.title %></h4>
                    <div class="signUp__form">
                        <!-- Begin MailChimp Signup Form -->
                        <div id="mc_embed_signup">
                        <form action="//reqfire.us13.list-manage.com/subscribe/post?u=c2e35a23f5bcaefe3f2657dfc&amp;id=<%-listId %>&amp;LANDPAGE=<%-pageName %>" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                            <div id="mc_embed_signup_scroll">
                        <!-- <div class="indicates-required"><span class="asterisk">*</span> indicates required</div> -->
                        <div class="mc-field-group">
                          <label for="mce-EMAIL">Email Address  <span class="asterisk">*</span>
                        </label>
                          <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL">
                        </div>
                        <div class="mc-field-group">
                          <label for="mce-FNAME">First Name </label>
                          <input type="text" value="" name="FNAME" class="" id="mce-FNAME">
                        </div>
                        <div class="mc-field-group">
                          <label for="mce-LNAME">Last Name </label>
                          <input type="text" value="" name="LNAME" class="" id="mce-LNAME">
                        </div>
                          <div id="mce-responses" class="clear">
                              <div class="response" id="mce-error-response" style="display:none"></div>
                              <div class="response" id="mce-success-response" style="display:none"></div>
                          </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                            <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_c2e35a23f5bcaefe3f2657dfc_<%-listId %>" tabindex="-1" value=""></div>
                            <div class="clear">
                                <input type="submit" value="<%-signUp.buttonText %>" name="subscribe" id="mc-embedded-subscribe" class="button" />
                              </div>
                            </div>
                        </form>
                        </div>
            
                        <!--End mc_embed_signup-->
                    </div>
                </div>
            </div>
          </div>
          <div class="content-container">
            <div class="primary__container">
                <div class="primary__titleGroup">
                    <h2><%-primary.title %></h2>
                    <p><%-primary.subPoint1 %></p>
                    <p><%-primary.subPoint2 %></p>
                </div>
                <div class="primary__img">
                    <img src="<%-primary.imgPath %>" alt="<%-primary.imgAlt %>">
                </div>
            </div>
            <div class="secondary__container">
                <div class="secondary__titleGroup--mobile">
                    <h2><%-secondary.title %></h2>
                    <p><%-secondary.subPoint1 %></p>
                    <p><%-secondary.subPoint2 %></p>
                </div>
                <div class="secondary__img">
                    <img src="<%-secondary.imgPath %>" alt="<%-secondary.imgAlt %>">
                </div>
                <div class="secondary__titleGroup">
                    <h2><%-secondary.title %></h2>
                    <p><%-secondary.subPoint1 %></p>
                    <p><%-secondary.subPoint2 %></p>
                </div>
            </div>
          </div>

            <div class="testimonial__fullBackground">
              <div class="testimonial__container">
                  <div class="testimonial__quoteBlock">
                      <blockquote>
                          &ldquo;
                          <%-testimonial[1].quote %>
                          <span><%-testimonial[1].quoteHighlight %></span>
                          &rdquo;
                          <br>
                          <img src="<%-testimonial[1].imgSrc %>" alt="<%-testimonial[1].imgAlt %>">
                      </blockquote>
                  </div>
                  <div class="testimonial__quoteBlock">
                      <blockquote>
                          &ldquo;
                          <span><%-testimonial[2].quoteHighlight %></span>
                          <%-testimonial[2].quote %>
                          &rdquo;
                          <br>
                          <img src="<%-testimonial[2].imgSrc %>" alt="<%-testimonial[2].imgAlt %>">
                      </blockquote>
                  </div>
              </div>
            </div>
            <div class="bannerCta__fullBackground">
              <div class="bannerCta__container">
                  <div class="bannerCta__ctaGroup--spacing"></div>
                  <div class="bannerCta__ctaGroup">
                      <h3>
                          <%-bannerCta.title %>
                      </h3>
                      <div class="bannerCta__button">
                          <a href="#waitingList"><%-bannerCta.buttonText %></a>
                      </div>
                  </div>
                  <div class="bannerCta__ctaGroup--spacing"></div>
              </div>
            </div>
           <div class="footer__fullBackground">
                <footer>
                   <ul class="footer__listSignup">
                       <li><a href="<%-footer.signUpLink %>">Sign Up</a></li>
                       <li><a target="_blank" href="/req/site/login">Sign In</a></li>
                       <li><a target="_blank" href="terms">Terms</a></li>
                   </ul>
                   <ul class="footer__listContact">
                       <li><a href="mailto:hello@reqfire.com">Contact: hello@reqfire.com</a></li>
                   </ul>
               </footer>
           </div>
           <div class="drag__background"></div>
          </div>
      </div>